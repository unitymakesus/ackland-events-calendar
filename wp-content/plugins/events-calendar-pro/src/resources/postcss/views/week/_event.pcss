.tribe-events-pro {

	.tribe-events-pro-week-grid__event {
		position: absolute;
		height: var(--spacer-5);
		top: 0;
		right: 0;
		width: 100%;

		/* If event is less than 1 hour */
		&:not([class*="tribe-events-pro-week-grid__event--h-"]) {

			.tribe-events-pro-week-grid__event-link-inner {
				padding: 2px var(--spacer-1);
			}

			.tribe-events-pro-week-grid__event-datetime {
				@mixin visually-hide;
			}

			.tribe-events-pro-week-grid__event-title-featured-icon {
				display: inline-block;
			}
		}
	}

	.tribe-events-pro-week-grid__event--featured {

		.tribe-events-pro-week-grid__event-link-inner {
			background-color: var(--color-accent-primary-week-event-featured);

			&:before {
				background: var(--color-accent-primary);
				content: '';
				height: 100%;
				left: 0;
				top: 0;
				position: absolute;
				width: 2px;
			}
		}

		.tribe-events-pro-week-grid__event-link {
			&:hover,
			&:focus {
				.tribe-events-pro-week-grid__event-link-inner {
					background-color: var(--color-accent-primary-week-event-featured-hover);
				}
			}
		}
	}

	.tribe-events-pro-week-grid__event--past {

		.tribe-events-pro-week-grid__event-link {
			background-color: var(--color-background-secondary);

			&:hover,
			&:focus {
				background-color: var(--color-background-secondary);
			}
		}
	}

	.tribe-events-pro-week-grid__event-link {
		background-color: var(--color-background);
		border: 2px solid var(--color-background);
		border-radius: var(--border-radius-default);
		height: 100%;
		overflow: hidden;
		position: absolute;
		width: 100%;

		&:hover,
		&:focus {
			z-index: var(--z-index-week-event-hover);

			.tribe-events-pro-week-grid__event-link-inner {
				background-color: var(--color-accent-primary-week-event-hover);
			}
		}
	}

	.tribe-events-pro-week-grid__event-link-inner {
		background-color: var(--color-accent-primary-week-event);
		display: block;
		height: 100%;
		padding: var(--spacer-0) var(--spacer-1);
		width: 100%;
	}

	/* -----------------------------------------------------------------------------
	 * Event Sequence
	 * ----------------------------------------------------------------------------- */

	.tribe-events-pro-week-grid__event--seq-2 {
		width: 90%;
	}

	.tribe-events-pro-week-grid__event--seq-3 {
		width: 80%;
	}

	.tribe-events-pro-week-grid__event--seq-4 {
		width: 70%;
	}

	.tribe-events-pro-week-grid__event--seq-5 {
