	/* -----------------------------------------------------------------------------
	 * Event details
	 * ----------------------------------------------------------------------------- */

	.tribe-events-pro-week-grid__event-title {
		flex: auto;
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
	}

	.tribe-events-pro-week-grid__event-datetime {
		@mixin mobile-body-3;

		color: var(--color-text-secondary);
		white-space: nowrap;

		> * {
			vertical-align: middle;
		}
	}

	.tribe-events-pro-week-grid__event-datetime-featured-icon,
	.tribe-events-pro-week-grid__event-datetime-recurring-icon {
		display: inline-block;
	}

	.tribe-events-pro-week-grid__event-title-featured-icon,
	.tribe-events-pro-week-grid__event-datetime-featured-icon {
		margin-right: 3px;
	}

	.tribe-events-pro-week-grid__event-recurring-icon {
		flex: none;
		margin-left: 5px;
	}

	.tribe-events-pro-week-grid__event-featured-image-wrapper {
		margin-bottom: var(--spacer-0);
	}

	.tribe-events-pro-week-grid__event-featured-image {
		padding-bottom: 56.25%;
	}


	/* -----------------------------------------------------------------------------
	 * Event Tooltip
	 * ----------------------------------------------------------------------------- */

	.tribe-events-pro-week-grid__event-tooltip {
		position: absolute;
		visibility: hidden; /* A11y purposes */
	}

	.tribe-events-pro-week-grid__event-tooltip-featured-image-wrapper {
		margin-bottom: var(--spacer-2);
		width: 206px;

		&:last-child {
			margin-bottom: 0;
		}
	}

	.tribe-events-pro-week-grid__event-tooltip-featured-image-link {
		display: block;
		transition: var(--transition-opacity);

		&:hover,
		&:focus {
			opacity: var(--opacity-icon-hover);
		}

		&:active {
			opacity: var(--opacity-icon-active);
		}
	}

	.tribe-events-pro-week-grid__event-tooltip-featured-image {
		padding-bottom: 56.25%;
	}

	.tribe-events-pro-week-grid__event-tooltip-description {
		margin-bottom: var(--spacer-1);

		&:last-child {
			margin-bottom: 0;
		}
	}

	.tribe-events-pro-week-grid__event-tooltip-cost {
		margin-top: var(--spacer-3);
	}
